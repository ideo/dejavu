{
  "type": "modal",
  "title": {
    "type": "plain_text",
    "text": "Search for Key Learnings",
    "emoji": true
  },
  "submit": {
    "type": "plain_text",
    "text": "Save",
    "emoji": true
  },
  "close": {
    "type": "plain_text",
    "text": "Cancel",
    "emoji": true
  },
  "blocks": [
    {
      "type": "input",
      "optional": false,
      "label": {
        "type": "plain_text",
        "text": "Related Themes (Required â€” you can choose multiple items)"
      },
      "element": {
        "type": "multi_static_select",
        "action_id": "predefinedRelatedThemeTags",
        "placeholder": {
          "type": "plain_text",
          "text": "Select related themes",
          "emoji": true
        },
        "options": []
      }
    },
    {
      "type": "input",
      "optional": true,
      "label": {
        "type": "plain_text",
        "text": "Industry"
      },
      "element": {
        "type": "static_select",
        "action_id": "predefinedIndustryTags",
        "placeholder": {
          "type": "plain_text",
